; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bound To Grow"
run/main_scene="res://scenes/intro/intro.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0.121569, 0.172549, 0.164706, 1)
boot_splash/fullsize=false
boot_splash/image="res://assets/ui/GAMETITLE.png"
boot_splash/minimum_display_time=1

[autoload]

GameEnums="*res://autoload/game_enums.gd"
GameEvents="*res://autoload/game_events.gd"
SceneTransition="*res://autoload/scene_transition.tscn"
GameData="*res://autoload/game_data.gd"
NotificationUI="*res://scenes/ui/notification_ui/notification_ui.tscn"
SoundManager="*res://autoload/sound_manager.tscn"
PauseScreen="*res://scenes/ui/pause_screen/pause_screen.tscn"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "orange",
"res://autoload/": "pink",
"res://resources/": "blue",
"res://scenes/": "teal"
}

